<StructureDefinition xmlns="http://hl7.org/fhir">
   <id value="ch-emed-medicationdispense-list"/>
   <url value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-medicationdispense-list"/>
   <version value="0.1.0"/>
   <name value="CHEMEDMedicationDispenseList"/>
   <title value="CH EMED MedicationDispense (LIST)"/>
   <status value="active"/>
   <experimental value="false"/>
   <description value="Definition of the medication dispense for the medication list document"/>
   <kind value="resource"/>
   <abstract value="false"/>
   <type value="MedicationDispense"/>
   <baseDefinition value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-medicationdispense"/>
   <derivation value="constraint"/>
   <differential>
      <element id="MedicationDispense">
         <path value="MedicationDispense"/>
         <short value="CH EMED MedicationDispense (LIST)"/>
      </element>

      <!-- parent document -->
      <element id="MedicationDispense.extension:parentDocument">
         <path value="MedicationDispense.extension" />
         <sliceName value="parentDocument" />
         <short value="Reference to the parent document"/>
         <min value="0"/>
         <max value="1"/>
         <type>
            <code value="Extension" />
            <profile value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-ext-dispense" />
         </type>
      </element>

      <!-- document author -->
      <element id="MedicationDispense.extension:authorDocument">
         <path value="MedicationDispense.extension" />
         <sliceName value="authorDocument" />
         <short value="Author of the original document if different from the author of the medical decision (MedicationDispense.performer.actor), see also 'Guidance - Different Authors'"/>
         <min value="0"/>
         <max value="1"/>
         <type>
            <code value="Extension" />
            <profile value="http://fhir.ch/ig/ch-core/StructureDefinition/ch-ext-author" />
         </type>
      </element>

      <!-- MedicationDispense Author -->
      <element id="MedicationDispense.performer">
         <path value="MedicationDispense.performer"/>
         <min value="1"/>
         <max value="1"/>
      </element>
      <element id="MedicationDispense.performer.actor">
         <path value="MedicationDispense.performer.actor"/>
         <short value="The author and her/his organization of the medical decision, see also 'Guidance - Different Authors'"/>
         <type>
            <code value="Reference"/>
            <targetProfile value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-practitionerrole"/>
         </type>
      </element>
      <element id="MedicationDispense.performer.actor.reference">
         <path value="MedicationDispense.performer.actor.reference"/>
         <min value="1"/>
      </element>
   </differential>
</StructureDefinition>     