<StructureDefinition xmlns="http://hl7.org/fhir">
   <id value="ch-emed-medicationrequest-list"/>
   <url value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-medicationrequest-list"/>
   <version value="0.1.0"/>
   <name value="CHEMEDMedicationRequestList"/>
   <title value="CH EMED MedicationRequest (LIST)"/>
   <status value="active"/>
   <experimental value="false"/>
   <description value="Definition of the medication request for the medication list document"/>
   <kind value="resource"/>
   <abstract value="false"/>
   <type value="MedicationRequest"/>
   <baseDefinition value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-medicationrequest"/>
   <derivation value="constraint"/>
   <differential>
      <element id="MedicationRequest">
         <path value="MedicationRequest"/>
         <short value="CH EMED MedicationRequest (LIST)"/>
      </element>

      <!-- document author -->
      <element id="MedicationRequest.extension:authorDocument">
         <path value="MedicationRequest.extension" />
         <sliceName value="authorDocument" />
         <short value="Author of the original document if different from the author of the medical decision (MedicationRequest.performer), see also 'Guidance - Different Authors'"/>
         <min value="0"/>
         <max value="1"/>
         <type>
            <code value="Extension" />
            <profile value="http://fhir.ch/ig/ch-core/StructureDefinition/ch-ext-author" />
         </type>
      </element>

      <!-- MedicationRequest Author -->
      <element id="MedicationRequest.performer">
         <path value="MedicationRequest.performer"/>
         <short value="The author and her/his organization of the medical decision, see also 'Guidance - Different Authors'"/>
         <min value="1"/>
         <type>
            <code value="Reference"/>
            <targetProfile value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-practitionerrole"/>
         </type>
      </element>
      <element id="MedicationRequest.performer.reference">
         <path value="MedicationRequest.performer.reference"/>
         <min value="1"/>
      </element>

   </differential>
</StructureDefinition>