<StructureDefinition xmlns="http://hl7.org/fhir">
   <id value="ch-emed-dosage-medicationrequest"/>
   <url value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-dosage-medicationrequest"/>
   <version value="2.0.0"/>
   <name value="CHEMEDDosageMedicationRequest"/>
   <title value="CH EMED Dosage (MedicationRequest)"/>
   <status value="active"/>
   <experimental value="false"/>
   <description value="Definition of the dosage (used in MedicationRequest)"/>
   <kind value="complex-type"/>
   <abstract value="false"/>
   <type value="Dosage"/>
   <baseDefinition value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-dosage"/>
   <derivation value="constraint"/>
   <differential>
      <element id="Dosage">
         <path value="Dosage"/>
         <short value="CH EMED Dosage (MedicationRequest)"/>
         <constraint>
            <key value="ch-dosage-3"/>
            <severity value="error"/>
            <human value="Free text dosage instruction, timing or as needed has to be specified"/>
            <expression value="text.exists() or timing.exists() or asNeededBoolean.exists()"/>
         </constraint>
      </element>
      <element id="Dosage.doseAndRate">
         <path value="Dosage.doseAndRate"/>
         <min value="1"/>
      </element>
   </differential>
</StructureDefinition>